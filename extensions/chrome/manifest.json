{
  "name": "Shelby Video Radar",
	"description": "Queue & Roll videos to Shelby.tv while you browse the web.",
  "version": "1.1.2",
  "manifest_version": 2,

	"icons": {
		 "16": "16.png",
		 "48": "48.png",
		"128": "128.png"
	},
	
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
			"exclude_matches": ["*://*.shelby.tv/*", "*://*.pandora.com/*"],
      "js": ["bootstrap.js"]
    }
  ]
}
