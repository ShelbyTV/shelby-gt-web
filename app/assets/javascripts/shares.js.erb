$(function(){

  Shelby.SharesNav = new Shelby.Navbar({
    el: $('.js-content-selector')
  });

  Shelby.SharesView = new Shelby.GuideView({
    el: $('.js-guide')
  });

  var loc = shelbyTrackingCategory || "Share Page",
      username = shelbyTrackingLabel || "Anonymous",
      apiRoot = '<%= "#{Settings::ShelbyAPI.protocol_matching_url}#{Settings::ShelbyAPI.version}" %>';

  shelby.trackEx({
    providers : ['ga'],
    gaCategory : loc,
    gaAction : 'Visit page',
    gaLabel : username
  });

});

