<h2 class="discussion__h2">
  To:
</h2>

<ul class="discussion__list discussion__list--recipients js-discussion-roll-recipients-list">
	<!-- content filled in by appending children: DiscussionRollRecipientView -->
</ul>
<a href="#" class="discussion__list--recipients-toggle-show-all js-discussion-roll-recipients-toggle-show-all">
	<span class="more">more...</span>
	<span class="less">[less]</span>
</a>

<% if(discussionRoll.has("content_updated_at")){ %>
  <span class="discussion__item--content-updated-at">
    <%= libs.shelbyGT.viewHelpers.app.summaryDate(new Date(discussionRoll.get("content_updated_at"))) %>
  </span>
<% } %>