<div id="dot-tv-welcome-message" class="notifications-wrapper animate_module">
  <h1>Welcome to <%= shelby.config.hostName %></h1>
  <div class="message">
    <%= roll.get('creator_nickname') %> curates this collection of video that is powered by Shelby.tv
  </div>
  <button class="button_primer button_blue dot-tv-welcome__button--play js-start-playing js-track-event" data-ga_category=".TV Welcome" data-ga_action="Click play button" data-ga_label="anonymous">
    <i class="button_icon_right icon_player_play icon">
      Start Watching
    </i>
  </button>
</div>