<div class="media_module user_roll__item_lining clearfix">
  <button class="button_primer<%= (customButtonColor) ? ' ' + customButtonColor : ' button_gray' %> media_image animate_module user-channel__button_prev js-button-previous">
    <i class="icon-arrow_left"></i>
    <span class="visuallyhidden">Previous</span>
  </button>

  <button class="button_primer<%= (customButtonColor) ? ' ' + customButtonColor : ' button_gray' %> media_image media_image--rev animate_module user-channel__button_next js-button-next">
    <i class="icon-arrow_right"></i>
    <span class="visuallyhidden">Next</span>
  </button>

  <div class="media_body user-channel__wrapper clearfix">
    <div class="user-channel__head">
      <h3 class="user-channel__title js-user-owned-roll-title">
        <% var rollTitle = rollTitleOverride || roll.get('title') %>
        <% if (showAttribution) { %>
          <%= rollTitle %> by <img class="user-channel__avatar" src="<%= attribution.authorAvatar %>"/ > <%= attribution.authorName %>
        <% } else { %>
          <%= rollTitle %>
        <% } %>
      </h3>
    </div>

    <div class="media_body user-channel__body js-user-owned-roll-wrapper">
      <div class="user-channel js-user-owned-roll clearfix">
        <!-- Filled in by â€¦ -->
      </div><!-- eo user-channel block -->
    </div><!-- eo media_body -->
</div><!-- eo media_module -->