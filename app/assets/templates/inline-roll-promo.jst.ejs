<section class="frame-list-item frame frame--inline-promo">
  <a class="frame-header" href="<%= linkSrc || '#' %>" onclick="return false;" title="<%= headerText %>">
    <div class="frame-body-wrapper clearfix">
      <div class="frame-body">
        <%=
          SHELBYJST['thumbnail-module']({
            thumbnail : avatarSrc,
            title : headerText
          })
        %>
      </div><!-- eo frame-body -->

      <div class="icon icon-chevron media_module frame-footer clearfix">
        <span class="image_module avatar media_image" style="background-image: url(<%= avatarSrc %>);" role="img">
          <img src="<%= avatarSrc %>" class="visuallyhidden" title="<%= 'username' %>" alt="<%= 'username' %>">
        </span>

        <div class="media_body">
          <p class="xuser-data">
            <%= 'username' %>
          </p>
          <p class="xuser-message">
            <%= headerText %>
          </p>
        </div><!-- eo media_body -->
      </div><!-- eo frame-footer -->
    </div><!-- eo frame-body-wrapper -->
  </a>
</section>
