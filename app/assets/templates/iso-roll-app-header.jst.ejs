
  <ol class="isoroll-nav__list">
    <% if(associatedRolls && currentRoll){ %>
      <% associatedRolls.forEach(function(roll,index){ %>
        <li class="isoroll-nav__item<%= (index == 0) ? ' isoroll-nav__item--parent' : ' isoroll-nav__item--child' %><%= roll.id == currentRoll.id ? ' isoroll-nav__item--active' : '' %>">

          <a class="isoroll-nav__button js-change-iso-roll" data-roll_id="<%= roll.id %>" href="#">
            <span class="isoroll-nav__avatar" style="background-image:url(<%= roll.get('thumbnail_url') %>);">
              <img src="<%= roll.get('thumbnail_url') %>" title="" alt="" class="visuallyhidden" />
            </span>

            <span class="isoroll-nav__title">
              <%= ((roll.get('roll_type') == libs.shelbyGT.RollModel.TYPES.special_public_real_user || roll.get('roll_type') == libs.shelbyGT.RollModel.TYPES.special_public_upgraded) && shelby.config.hostName) ? shelby.config.hostName : roll.get('title') %>
            </span>
          </a>
        </li>
      <% }) %>
    <% } %>

    <% if (currentRoll.has('creator_nickname')) { %>
      <li class="isoroll-nav__item left-secondary">
        <button class="button_primer button_gray isoroll-nav__button--shelby js-subscribe-button">
          <span class="icon icon-email"></span> Subscribe via Emailâ€¦
        </button>
      </li>

      <li class="isoroll-nav__item secondary">
        <button class="button_primer button_gray-seven isoroll-nav__button--shelby js-full-shelby-button">reserve your own .tv</button>
      </li>

      <li class="isoroll-nav__item secondary">
        <button class="button_primer button_gray-seven isoroll-nav__button--unfollow js-follow-button">&nbsp;</button>
      </li>
    <% } %>
  </ol>
