<% var video = frame.get('video') %>
<% var videoId = frame.get('id') %>
<% var videoTitle = video.get('title') %>
<% var videoThumbnail = video.get('thumbnail_url') %>
<article id="<%= videoId %>" class="explore-frame clearfix transparent">
  <ul class="menu_list frame-command-menu xframe-list" role="menubar">
    <li class="menu_item" role="menuitem">
      <button class="button_primer button_gray button_command button_command--roll js-roll-command js-track-event" data-ga_category="User Profile" data-ga_action="Click to roll frame" data-ga_label="<%= videoTitle %>">
        <span class="icon icon-roll"></span>
        <span class="button_label">Shareâ€¦</span>
      </button>
    </li>

    <li class="menu_item" role="menuitem">
      <button class="button_primer button_gray button_command button_command--like js-queue-command js-track-event" data-ga_category="User Profile" data-ga_action="Click to like frame" data-ga_label="<%= videoTitle %>">
        <span class="icon icon-like js-command-icon"></span>
        <span class="label">Like</span>
      </button>
    </li>
  </ul>

  <a href="#" onclick="return false;" class="image_module frame-thumbnail js-play-explore-frame<%= (!videoThumbnail) ? ' missing-thumbnail' : '' %> js-track-event" data-ga_category="User Profile" data-ga_action="Click thumbnail to play frame" data-ga_label="<%= videoTitle %>" style="background-image: url('<%= videoThumbnail %>');" role="img">
    <img class="visuallyhidden" src="<%= videoThumbnail %>" title="<%= videoTitle %>" alt="<%= videoTitle %> thumbnail" onload="$(this).parents('.explore-frame').first().removeClass('transparent');">
  </a>

  <a href="#" onclick="return false;" class="explore-frame-link js-play-explore-frame js-track-event" data-ga_category="User Profile" data-ga_action="Click title to play frame" data-ga_label="<%= videoTitle %>">
    <h3 class="explore-frame-title">
      <%= videoTitle %>
    </h3>
  </a>

  <% if (showFrameLikes) { %>
    <div class="frame-likes frame-external-subline icon-like icon-like--red js-frame-likes">
      <!-- Filled in by the FrameLikesView -->
    </div><!-- eo frame-likes -->
  <% } %>
</article>