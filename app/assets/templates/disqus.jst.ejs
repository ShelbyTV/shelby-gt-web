<%
	window.DISQUS = null;
	$('#disqus-script').remove();
	// loading 'special' disqus forums for some people
	if (frame.get('creator').get('nickname') == "henry"){
		window.disqus_shortname = "henrytv";
	}
	else if (frame.get('creator').get('nickname') == "fredwilson"){
		window.disqus_shortname = "fredwilsontv";
	}
	else if (frame.get('creator').get('nickname') == "reece"){
		window.disqus_shortname = "reecepachecotv";
	}
	window.disqus_developer = (shelby.config.environment == "production") ? 0 : 1; // developer mode is on
	window.disqus_identifier = frame.id;
	window.disqus_url = "http://shelby.tv/frame/"+frame.id;
	window.disqus_title = "Shelby.tv | "+frame.get('video').get('title');
	(function() {
	    var dsq = document.createElement('script'); dsq.type = 'text/javascript'; dsq.async = true;
	    dsq.src = 'http://shelbytv.disqus.com/embed.js';
			dsq.id = 'disqus-script';
	    (document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(dsq);
	})();
%>