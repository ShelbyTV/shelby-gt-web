<div class="shelf shelf--community clearfix">
  <div class="shelf__lining">
    <h3 class="shelf__h2">
      Start watching .TV now.
    </h3>

    <ul class="list shelf__list clearfix">
      <% (0..5).each do |i| %>
        <li class="shelf__item">
          <article class="frame shelf__frame">
            <a href="#link-to-roll" class="shelf__link" title="watch username's roll">
              <div class="frame-body-wrapper">
                <div class="image_module frame-thumbnail" style="background-image: url('http://b.vimeocdn.com/ts/395/986/395986225_1280.jpg');" role="img">
                  <img class="visuallyhidden" src="http://b.vimeocdn.com/ts/395/986/395986225_1280.jpg" title="The Fort - Sundance 2012" alt="The Fort - Sundance 2012 thumbnail">
                </div><!-- eo .frame-thumbnail -->

                <div class="media_module frame-footer clearfix">
                  <div class="media_body">
                    <h3 class="xuser-data">
                      vincentvondoom.shelby.tv
                    </h3>

                    <p class="xuser-message">
                      Singer/rapper Baby Kaely teams up with @iamwill for a tribute to lives lost in the Newtown Massacre. http://t.co/9ZkSCu8Y
                    </p>
                  </div><!-- eo .media_body -->
                </div>
              </div><!-- eo .frame-body-wrapper -->
            </a>
          </article>
        </li>
      <% end %>
    </ul>
  </div><!-- eo shelf__lining -->
</div><!-- eo shelf -->