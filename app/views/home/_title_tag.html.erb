<% if @frame and @video %>
  <% if @frame['creator'] %>
    <%= @video['title'] %> shared by <%= "#{@frame['creator']['name']} (#{@frame['creator']['nickname']})" %> on Shelby.tv
  <% else %>
    <%= @video['title'] %> on Shelby.tv
  <% end %>

<% elsif @roll %>
	<%= page_title_for_roll(@roll, @user) %>

<% else %>
	<%= Settings::Application.title %>

<% end %>