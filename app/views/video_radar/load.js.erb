            styleGuideHide: function (guideName) {
                if (!shelbyJ("#<%=@prefix%>frame-guide-window").hasClass("guide-show")) {
                    shelbyJ('.<%=@prefix%>guide-toggle').removeClass("hide").addClass("show");
