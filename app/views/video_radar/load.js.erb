                                    utilObject.resizeFunction(vids, vidIndex, r);
                                }
                            }(i, r);
                            shelbyJ("body").on('webkitTransitionEnd transitionend msTransitionEnd oTransitionEnd', function (event) {
                                resizeFunction();
                            });
                            utilObject.resizeArray.push(resizeFunction);
                        }

