                var width = utilObject.getWidth(vid.embed);
                shelbyJ('#<%=@prefix%>control-on-page-' + r).css("left", utilObject.getFooterHpos(vid.embed.offset().left)).css("width", width).css("top", utilObject.getFooterVpos(vid.embed.offset().top));
