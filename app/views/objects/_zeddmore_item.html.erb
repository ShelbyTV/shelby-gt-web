<%#

  NOTE:

  This partial is currently Zeddmore specific.

%>

<article id="" data-video_id="<%= video['video_id'] %>" class="animate_module frame frame-list-item frame-collapsed js-open-item">
  <div class="frame-body-wrapper clearfix js-frame-activate">
    <div class="frame-body">
      <span class="image_module frame-thumbnail js-frame-activate" style="background-image: url('<%= video['thumbnail_url'] %>');" role="img">
        <img class="visuallyhidden" src="<%= video['thumbnail_url'] %>" title="<%= video['title'] %>" alt="<%= video['title'] %> thumbnail">
      </span>
      <div class="xvideo-unplayable-thumbnail-overlay">⏏<br>unplayable</div>
    </div><!-- eo .frame-body -->

    <footer class="media_module frame-footer clearfix">
      <div class="media_body">
        <span class="frame-header js-frame-activate">
          <h1 class="frame-title" title="<%= video['title'] %>">
           <%= video['title'] %>
          </h1>
          <div class="xuser-data">
            <%= raw "#{Time.at(video['duration'].to_i).gmtime.strftime('%M:%S')} | #{video['provider_name']}" %>
          </div>
        </span>
      </div>
    </footer><!-- eo .media_module-->
  </div><!-- eo .frame-body-wrapper -->
  <div class="progress frame-body-wrapper"><div style="width: <%= raw "#{video['count_as_ratio'] * 100 * 4}" %>%"></div></div>
</article>
