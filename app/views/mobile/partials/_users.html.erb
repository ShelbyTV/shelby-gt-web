<ul class="list">
  <% users.each do |user| %>
    <% if [Settings::RollTypes.special_public_real_user, Settings::RollTypes.special_public_upgraded].include?(user['roll_type']) and (user['creator_nickname'] != signed_in_user['nickname']) %>
      <li class="list__item" data-user="<%= "#{user}" %>">
        <%= render :partial => '/objects/user_card',
            :locals => {
              :avatar       => avatar_url_for_roll(user),
              :clickable    => true,
              :ga_category  => ga_category,
              :is_following => is_following,
              :realname     => user['creator_name'],
              :roll_id      => user['id'],
              :username     => user['creator_nickname'],
              :user_type    => user['user_type']
            } %>
      </li>
    <% end %>
  <% end %>
</ul>
