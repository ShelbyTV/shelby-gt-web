<ul class="list js-list">
  <% @dashboard.each do |dbe| %>
    <li class="list__item">
      <% if dbe['frame'] and dbe['frame']['video'] and dbe['action'] != 31 %>
        <%= render :partial => '/objects/frame',
            :locals => {
              :frame          => dbe['frame'],
              :frame_owner    => dbe['frame']['creator'],
              :video          => dbe['frame']['video'],
              :signed_in_user => @signed_in_user,
              :is_mobile      => true,
              :embed_video    => false
            } %>
      <% end %>
    </li>
  <% end %>
</ul>

<p class="center">
  <%= link_to mobile_stream_path(:page => (@page + 1)), {:class => Settings::Mobile.button_gray_flexible + " one-third js-load-more"} do %>
    <span class="button_label">Load More</span>
  <% end %>
</p>
