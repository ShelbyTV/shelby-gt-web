<div id="mobile-video-player"></div>

<div id="mobile-frames">
	<ul id="rolls-frame-list" data-role="listview" data-theme="c">
		<% @frames.each do |f| %>
			<li class="<%= f['video']['provider_name'] %>-<%= f['video']['provider_id'] %> mobile-frame">
				<%= render :partial => "mobile_frame", :locals => { :frame => f } %>
			</li>
		<% end %>
	</ul>
</div>

<script>
	$('h1').text("The <%= @roll['title'].titlecase %> Roll");
</script>