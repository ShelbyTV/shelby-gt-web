<% Object.keys(frame).forEach(function(k){ %>
  <% if (active) {%>
    <span style="text-decoration:underline">
  <% } %>
	  <% if (k==='roll') {%>
	    <span class="roll">
	  <% } %>

	  <%= k+':'+frame[k] %>

	  <% if (k==='roll') {%>
	    </span>
	  <% } %>
